#include<stdio.h>

int main()
{
    int T,N,i;
    scanf("%d",&T);
    if(T>=1 && T<=100)
    {
        for(i=1;i<=T;i++)
        {
            scanf("%d",&N);
            if(N>=1 && N<=100)
            {
                N=(N*2)-1;
                printf("Case #%d: %d\n",i,N);
            }
        }
    }

    return 0;
}
