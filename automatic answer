#include<stdio.h>

int main()
{
   int t,n,i;
   double N;
   scanf("%d",&t);
   if(t>=1 && t<=100)
   {
       for(i=1;i<=t;i++)
       {
           scanf("%d",&n);
           if(n>=-1000 && n<=1000)
           {
               n=n*567;
               n=n/9;
               n=n+7492;
               n=n*235;
               n=n/47;
               n=n-498;
               N=n/100.0;
               n=n/100;
               n=(N-n)*10;
               if(n>0)
               {
                   printf("%d\n",n);
               }
               else
               {
                   printf("%d\n",-n);
               }

           }
       }
    }

   return 0;
}
